<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<VBox prefHeight="618.0" prefWidth="750.0" style="-fx-font-size: 18px;" stylesheets="@../css/list.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jason.use.controller.UserController">
  <children>
  <AnchorPane prefHeight="616.0" prefWidth="713.0" style="-fx-background-color: lightblue;">
     <children>
        <VBox layoutY="1.0" prefHeight="612.0" prefWidth="775.0">
           <children>
              <HBox prefHeight="59.0" prefWidth="776.0" spacing="10.0" style="-jfx-cell-horizontal-margin: 10;">
                 <children>
                    <JFXTextField promptText=" 编号/分享用户/小号名" style="-fx-background-color: white;" fx:id="searchName"/>
                    <JFXButton text="搜索" onAction="#searchUserList"/>
                    <JFXButton text="刷新" onAction="#refreshUserList"/>
                 </children>
                 <padding>
                    <Insets top="10.0" />
                 </padding>
              </HBox>
              <VBox prefHeight="552.0" prefWidth="775.0">
                 <children>
                     <JFXTreeTableView fx:id="userList" prefHeight="474.0" prefWidth="745.0" style="-fx-font-size: 16px;">
                         <columns>
                             <JFXTreeTableColumn fx:id="id" editable="false" minWidth="62.0" prefWidth="62.0" resizable="false" sortable="false" text="编号" />
                             <JFXTreeTableColumn fx:id="userName" editable="false" minWidth="98.0" prefWidth="158.0" resizable="false" sortable="false" text="分享用户" />
                             <JFXTreeTableColumn fx:id="subUserName" editable="false" minWidth="39.0" prefWidth="160.0" resizable="false" sortable="false" text="小号名" />
                             <JFXTreeTableColumn fx:id="replaceOrderTimes" editable="false" minWidth="48.0" prefWidth="113.0" resizable="false" sortable="false" text="日-日上限" />
                             <JFXTreeTableColumn fx:id="connectionStatus" editable="false" minWidth="28.0" prefWidth="115.0" resizable="false" sortable="false" text="连接状态" />
                             <JFXTreeTableColumn fx:id="location" editable="false" minWidth="35.0" prefWidth="135.0" resizable="false" sortable="false" text="地理位置" />
                             <JFXTreeTableColumn fx:id="subUserId" visible="false" editable="false" minWidth="0" prefWidth="0" resizable="false" sortable="false" text="sub user id" />
                         </columns>
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                     </JFXTreeTableView>
                        <HBox prefHeight="42.0" prefWidth="206.0">
                           <VBox.margin>
                              <Insets bottom="15.0" top="15.0" />
                           </VBox.margin>
                           <children>
                              <Label prefHeight="42.0" prefWidth="215.0" text="第1/10页，共98条记录" textAlignment="CENTER" fx:id="pageRecord"/>
                              <Label prefHeight="42.0" prefWidth="57.0" text="转到：" />
                              <JFXTextField fx:id="searchPage" prefHeight="42.0" prefWidth="49.0" style="-fx-background-color: white; -fx-border-color: white;" />
                              <Label alignment="CENTER" prefHeight="42.0" prefWidth="60.0" text="页" />
                              <JFXButton prefHeight="52.0" prefWidth="83.0" text="Go!" onAction="#searchPage"/>
                              <JFXButton prefHeight="42.0" prefWidth="83.0" text="上一页" onAction="#previousPage">
                                 <HBox.margin>
                                    <Insets left="10.0" right="10.0" />
                                 </HBox.margin>
                              </JFXButton>
                              <JFXButton prefHeight="45.0" prefWidth="83.0" text="下一页"  onAction="#nextPage"/>
                           </children>
                        </HBox>
                 </children>
              </VBox>
           </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets left="10.0" right="20.0" />
               </padding>
        </VBox>
     </children>
      <StackPane fx:id="stackPane" prefHeight="618.0" prefWidth="750.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
  </AnchorPane>
  </children>
</VBox>
