<div class="modal fade" id="confirmModal">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<h4 class="modal-title" id="confirmModalTitle"></h4>
			</div>
			<div class="modal-body">
				<p id="confirmModalMessage"></p>
			</div>
			<div class="modal-footer">
				<input type="button" class="btn btn-warning" data-dismiss="modal" id="confirmModalBtn"/>
				<input type="button" class="btn btn-info" data-dismiss="modal" id="confirmModalCalBtn"/>
			</div>
		</div>
	</div>
</div>
<div class="modal fade" id="promptModal">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<h4 class="modal-title" id="promptModalTitle"></h4>
			</div>
			<div class="modal-body">
				<textarea id="promptModalMessage" class="form-control" style="display: block;width: 80%;resize:none;"></textarea>
			</div>
			<div class="modal-footer">
				<input type="button" class="btn btn-warning" data-dismiss="modal" id="promptModalBtn"/>
				<input type="button" class="btn btn-info" data-dismiss="modal" id="promptModalCalBtn"/>
			</div>
		</div>
	</div>
</div>
<div class="modal fade" id="alertModal">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<h4 class="modal-title" id="modalTitle"></h4>
			</div>
			<div class="modal-body">
				<p id="modalMessage"></p>
			</div>
			<div class="modal-footer">
				<input type="button" class="btn btn-warning" data-dismiss="modal" id="modalBtn"/>
			</div>
		</div>
	</div>
</div>